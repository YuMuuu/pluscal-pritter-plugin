  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^20.12.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "type": "module",
  "dependencies": {
    "@types/antlr4": "^4.11.6",
    "antlr4": "^4.13.1-patch-1",
    "prettier": "^3.2.5"
  },
  "exports": {
    ".": {
      "types": "./src/antlr4/index.d.ts"
    }
  }
